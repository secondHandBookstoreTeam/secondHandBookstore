/**app.wxss**/
@import "./styles/iconfont/iconfont.wxss";
page,view,text,swiper,swiper-item,image,navigator{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
/*主题颜色,通过变量来实现*/
page{
    /*定义主题颜色*/
    --themeColor:#eb4450;
    /*定义统一字体大小*/
    font-size: 28rpx;
}

image{
    width: 100%;
}