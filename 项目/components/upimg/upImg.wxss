/* components/upimg/upImg.wxss */
.up_img_wrap {
  width: 100rpx;
  height: 95rpx;
  position: relative;
  margin: 10rpx 15rpx; }
  .up_img_wrap image {
    width: 100%;
    height: 100%;
    border-radius: 10rpx; }
  .up_img_wrap .clear {
    position: absolute;
    width: 40rpx;
    height: 40rpx;
    background-color: red;
    color: white;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    top: -18rpx;
    right: -20rpx; }
